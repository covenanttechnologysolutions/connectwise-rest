{
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 2020
  },
  "plugins": ["@typescript-eslint", "prettier"],
  "env": {
    "node": true,
    "mocha": true,
    "es6": true
  },
  "extends": [
    "plugin:@typescript-eslint/eslint-recommended",
    "plugin:@typescript-eslint/recommended",
    "plugin:prettier/recommended"
  ],
  "rules": {},
  "ignorePatterns": [
    "dist",
    "generator/Manage",
    "generator/Automate",
    "src/AutomateTypes.js",
    "src/ManageTypes.js"
  ]
}
